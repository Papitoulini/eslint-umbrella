{
  "name": "@eslint-umbrella/presets",
  "version": "1.1.0",
  "type": "module",
  "description": "Pinned ESLint flat config presets for React, Next.js, Express, NestJS, React Native, React-PDF, and base/test",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/index.js",
    "./base": "./src/base.js",
    "./base-typechecked": "./src/base-typechecked.js",
    "./react": "./src/react.js",
    "./next": "./src/next.js",
    "./express": "./src/express.js",
    "./nest": "./src/nest.js",
    "./react-native": "./src/react-native.js",
    "./react-pdf": "./src/react-pdf.js"
  },
  "sideEffects": false,
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ci": "eslint . --max-warnings=0",
    "changeset": "changeset",
    "version-packages": "changeset version && npm i --package-lock-only --ignore-scripts",
    "release": "changeset publish"
  },
  "license": "MIT",
  "dependencies": {
    "@next/eslint-plugin-next": "14.2.14",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-react": "7.37.1",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.12",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "^16.4.0",
    "typescript": "5.6.3",
    "typescript-eslint": "8.8.1",
    "eslint-plugin-promise": "^7.2.1",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@eslint/js": "^9.35.0",
    "eslint-plugin-react-native": "^5.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "eslint": "^9.35.0",
    "nodemon": "^3.1.10"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  }
}
